@using PersonerosWeb.Models;
@model TipoEleccion
@{
    ViewBag.Title = "Detalle del Tipo de Eleccion";
    Layout = null;
}

<div class="card">
    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>ID</dt>
            <dd>@Model.idTipoEleccion</dd>
            <dt>Nombre</dt>
            <dd>@Model.nombre</dd>
            <dt>Candidato</dt>
            <dd>@Model.personaCandidato.nombres @Model.personaCandidato.apellidoPaterno @Model.personaCandidato.apellidoMaterno</dd>
        </dl>
    </div>
</div>