@using PersonerosWeb.Models;
@model TipoUsuario
@{
    ViewBag.Title = "Detalle del Tipo de Usuario";
    Layout = null;
}

<div class="card">
    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>ID</dt>
            <dd>@Model.idTipoUsuario</dd>
            <dt>Nombres</dt>
            <dd>@Model.nombre</dd>
            <dt>Identificador</dt>
            <dd>@Model.identificador</dd>
            <dt>Nivel de Acceso</dt>
            <dd>@Model.nivelAcceso</dd>
        </dl>
    </div>
</div>